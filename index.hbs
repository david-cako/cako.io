{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}
<meta name="google-site-verification" content="CZuJBVbmjQdYaKUXQ6pDcDQ1Y9dH13s1oeZmN_AZpu4" />

<header class="site-header outer">
    {{> "site-nav"}}
</header>
{{!-- The big featured header, it uses blog cover image as a BG if available --}}
{{!-- The main content area --}}
<main id="site-main" class="site-main outer" role="main">
     <div class="inner" style="position: relative">
       <div id="cako-post-feed-outer">
           <div id="cako-intro">A blog and software consultancy by <a id="name-link" href="https://davidcako.com">David Cako</a></div>
           <div id="cako-post-feed">
                {{#foreach posts}}

                    {{!-- The tag below includes the markup for each post - partials/post-card.hbs --}}
                    {{> "post-card"}}

                {{/foreach}}
            </div>
        </div>
    </div>
</main>
